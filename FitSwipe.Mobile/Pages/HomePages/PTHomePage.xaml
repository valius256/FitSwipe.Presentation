<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitSwipe.Mobile.Pages.HomePages.PTHomePage"
             xmlns:controls="clr-namespace:FitSwipe.Mobile.Controls">
    <AbsoluteLayout BackgroundColor="White">
        <Label 
            Text="This is home page for PT"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <controls:NavbarPT
            x:Name="navbar"
            ActiveTab="1"
            AbsoluteLayout.LayoutBounds="0,1,1,AutoSize" 
            AbsoluteLayout.LayoutFlags="PositionProportional, WidthProportional" ></controls:NavbarPT>
    </AbsoluteLayout>
</ContentPage>